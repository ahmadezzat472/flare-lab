{
  "version": 2,
  "builds": [
    {
      "src": "dist/spa/**",
      "use": "@vercel/static"
    },
    {
      "src": "server/**",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/spa/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "GENERATE_SOURCEMAP": "false"
  }
}
